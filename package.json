{
	"name": "jarvis-teamspeak-bot",
	"version": "0.9.3",
	"description": "Jarvis is an extensible Teamspeak 3 bot written in Javascript. Can be extended by building your own plugins!",
	"main": "app.js",
	"scripts": {
		"jarvis": "node app.js",
		"start": "node app.js",
		"dev": "nodemon --ignore config/ app.js",
		"debug": "nodemon --ignore config/ --inspect app.js",
		"firebase": "nodemon helpers/firebaseHelper.js"
	},
	"keywords": [
		"teamspeak",
		"ts3",
		"bot",
		"modular",
		"plugins",
		"ssh",
		"raw"
	],
	"author": "Jeremy Paton",
	"license": "MIT",
	"dependencies": {
		"async": "^3.1.0",
		"csgo": "^1.5.14",
		"firebase-admin": "^8.6.0",
		"node-fetch": "^2.6.0",
		"slackbots": "^1.2.0",
		"steam": "^1.4.1",
		"ts3-nodejs-library": "^2.0.3-a"
	},
	"devDependencies": {
		"nodemon": "^1.19.2"
	}
}
